<?xml version="1.0"?>
<ui>
    <Panel>
        <x>10</x>
        <y>10</y>
        <width>settings.getWidth() - 20</width>
        <height>settings.getHeight() - 80</height>
        <childs>
            <Label>
                <x>settings.getWidth() / 2 - 150</x>
                <y>10</y>
                <width>300</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Map Filter Settings")</text>
                <font>main24</font>
            </Label>
            <Label>
                <x>10</x>
                <y>lastY + lastHeight + 20</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Map name:")</text>
                <font>main24</font>
            </Label>
            <Textbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>400</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Name or part of the map name.")</tooltip>
                <startValue>return mapSelectionFilter.getMapName()</startValue>
                <onEvent></onEvent>
            </Textbox>
            <Label>
                <x>10</x>
                <y>lastY + lastHeight + 10</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Author:")</text>
                <font>main24</font>
            </Label>
            <Textbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>400</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Name or part of the author.")</tooltip>
                <startValue>return mapSelectionFilter.getMapAuthor()</startValue>
                <onEvent></onEvent>
            </Textbox>
            <Label>
                <x>10</x>
                <y>lastY + lastHeight + 10</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Player:")</text>
                <font>main24</font>
            </Label>
            <Spinbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Minimum amount of players for the map")</tooltip>
                <startValue>return mapSelectionFilter.getMinPlayer()</startValue>
                <infinite>0</infinite>
                <min>0</min>
                <max>999999</max>
                <onEvent>mapSelectionFilter.setMinPlayer(value)</onEvent>
            </Spinbox>
            <Label>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>10</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","-")</text>
                <font>main24</font>
            </Label>
            <Spinbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Maximum amount of players for the map")</tooltip>
                <startValue>return mapSelectionFilter.getMaxPlayer()</startValue>
                <infinite>0</infinite>
                <min>0</min>
                <max>999999</max>
                <onEvent>mapSelectionFilter.setMaxPlayer(value)</onEvent>
            </Spinbox>
            <Label>
                <x>10</x>
                <y>lastY + lastHeight + 10</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Width:")</text>
                <font>main24</font>
            </Label>
            <Spinbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Minimum width of the map")</tooltip>
                <startValue>return mapSelectionFilter.getMinWidth()</startValue>
                <infinite>0</infinite>
                <min>0</min>
                <max>999999</max>
                <onEvent>mapSelectionFilter.setMinWidth(value)</onEvent>
            </Spinbox>
            <Label>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>10</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","-")</text>
                <font>main24</font>
            </Label>
            <Spinbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Maximum width of the map")</tooltip>
                <startValue>return mapSelectionFilter.getMaxWidth()</startValue>
                <infinite>0</infinite>
                <min>0</min>
                <max>999999</max>
                <onEvent>mapSelectionFilter.setMaxWidth(value)</onEvent>
            </Spinbox>
            <Label>
                <x>10</x>
                <y>lastY + lastHeight + 10</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Height:")</text>
                <font>main24</font>
            </Label>
            <Spinbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Minimum height of the map")</tooltip>
                <startValue>return mapSelectionFilter.getMinHeight()</startValue>
                <infinite>0</infinite>
                <min>0</min>
                <max>999999</max>
                <onEvent>mapSelectionFilter.setMinHeight(value)</onEvent>
            </Spinbox>
            <Label>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>10</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","-")</text>
                <font>main24</font>
            </Label>
            <Spinbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <tooltip>QT_TRANSLATE_NOOP("GAME","Maximum height of the map")</tooltip>
                <startValue>return mapSelectionFilter.getMaxHeight()</startValue>
                <infinite>0</infinite>
                <min>0</min>
                <max>999999</max>
                <onEvent>mapSelectionFilter.setMaxHeight(value)</onEvent>
            </Spinbox>
            <Label>
                <x>10</x>
                <y>lastY + lastHeight + 10</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Bases:")</text>
                <font>main24</font>
            </Label>
            <Checkbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <tooltip>QT_TRANSLATE_NOOP("GAME","If the map needs to have bases.")</tooltip>
                <startValue>mapSelectionFilter.addToFilter(GameEnums.MapFilterFlags_Bases, mapSelectionFilter.isFlagActive(GameEnums.MapFilterFlags_Bases))</startValue>
                <onEvent>mapSelectionFilter.addToFilter(GameEnums.MapFilterFlags_Bases, value);</onEvent>
            </Checkbox>
            <Label>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <width>150</width>
                <height>30</height>
                <text>QT_TRANSLATE_NOOP("GAME","Optional:")</text>
                <font>main24</font>
            </Label>
            <Checkbox>
                <x>lastX + lastWidth + 10</x>
                <y>lastY</y>
                <tooltip>QT_TRANSLATE_NOOP("GAME","If the map needs to have bases.")</tooltip>
                <startValue>mapSelectionFilter.addToFilter(GameEnums.MapFilterFlags_Bases, mapSelectionFilter.isFlagOption(GameEnums.MapFilterFlags_Bases))</startValue>
                <onEvent>mapSelectionFilter.addToFilter(GameEnums.MapFilterFlags_Bases, value);</onEvent>
            </Checkbox>
        </childs>
    </Panel>
    <Button>
        <x>settings.getWidth() / 2 - 75</x>
        <y>settings.getHeight() - 60</y>
        <width>150</width>
        <text>QT_TRANSLATE_NOOP("GAME","Ok")</text>
        <tooltip>QT_TRANSLATE_NOOP("GAME","Closes the map filter dialog")</tooltip>
        <onEvent>mapSelectionFilter.exit();</onEvent>
    </Button>
</ui>
